{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Items"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Product_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "BrandName": {
      "type": "string",
      "required": true
    },
    "Individual_category": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Price": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Product_id"
    },
    "Image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
